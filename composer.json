{
  "name": "shanwker1223/yii2-jwt",
  "description": "JWT based on Icobucci",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "yii 2",
    "jwt"
  ],
  "authors": [
    {
      "name": "Gabriel Lucas",
      "email": "gabriellucasdev@gmail.com"
    },
    {
      "name": "Dmitriy Demin",
      "email": "sizemail@gmail.com",
      "homepage": "https://sizeg.tk"
    }
  ],
  "license": "BSD-3-Clause",
  "require": {
    "php": "^8.1",
    "lcobucci/jwt": "~4.2.1",
    "yiisoft/yii2": "~2.0.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "shanwker1223\\jwt\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "shanwker1223\\jwt\\tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
